{
  "extends": [
    "development"
  ],
  "hints": {
    "button-type": "off",
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "overflow-wrap: anywhere",
          "backdrop-filter"
        ]
      }
    ],
    "meta-viewport": "off",
    "highest-available-document-mode": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-progressbar-name": "off"
      }
    ]
  }
}